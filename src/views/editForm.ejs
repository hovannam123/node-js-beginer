<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Edit user</title>
</head>

<body>
    <form action="/update-user" method="POST">
        <label>First Nameeeee</label>
        <input type="text" name="firstName" value="<%= dataUser.firstName %>" />

        <label>Last Name</label>
        <input type="text" name="lastName" value="<%= dataUser.lastName %>">

        <label>Email</label>
        <input type="text" name="email" value="<%= dataUser.email %>">

        <label>Phone Number</label>
        <input type="text" name="phoneNumber" value="<%= dataUser.phoneNumber %>">
        <input type="text" value="<%= dataUser.id %>" hidden name="id">

        <button type="submit">Edit</button>
    </form>
</body>

</html>